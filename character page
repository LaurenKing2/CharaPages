        <!-- muse 10 tab 2 starts -->
        <div id="tab10b" class="subtab">
          <div class="bio-title">Bloodthirsty Hero (Vampire AU)</div>
          <div class="quick-points">
            <div class="left-side">
              <div class="info">
                <div class="marker">Fandom</div>
                <span>BnHA</span>
              </div>
              <br>
              <div class="info">
