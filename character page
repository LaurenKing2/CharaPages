        <!-- muse 13 tab 1 starts -->
        <div id="tab13a" class="subtab active">
          <div class="bio-title">Chiefly By My Villainy (Regular AU)</div>
          <div class="quick-points">
            <div class="left-side">
              <div class="info">
                <div class="marker">Fandom</div>
                <span>BnHA</span>
              </div>
              <div class="info">
