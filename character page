        <!-- muse 3 tab 1 starts -->
        <div id="tab3a" class="subtab active">
          <div class="bio-title">TodoFam Drama (Regular AU)</div>
          <div class="quick-points">
            <div class="left-side">
              <div class="info">
                <div class="marker">Fandom</div>
