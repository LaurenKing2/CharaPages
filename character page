        <!-- muse 1 tab 1 starts -->
        <div id="tab1a" class="subtab active">
          <div class="bio-title">Plus Ultra!! (Regular AU)</div>
          <div class="quick-points">
            <div class="left-side">
              <div class="info">
                <div class="marker">Fandom</div>
                <span>BnHA</span>
              </div>
              <br>
              <div class="info">
