        <!-- muse 6 tab 2 starts -->
        <div id="tab6b" class="subtab">
          <div class="bio-title">Cat's In The Cradle (Pro Hero AU)</div>
          <div class="quick-points">
            <div class="left-side">
              <div class="info">
                <div class="marker">Fandom</div>
                <span>BnHA</span>
